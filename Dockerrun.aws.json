{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "commuters",
            "image": "241619/commuters",
            "hostname": "commuters",
            "essential": false,
            "memory": 128,
            "command": [
                "tail",
                "-f",
                "/bin/bash"
            ]
        },
        {
            "name": "nginx-custom",
            "image": "241619/docker-basics-nginx-custom",
            "hostname": "proxy",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["commuters"],
            "memory": 128,
            "command": [
                "tail",
                "-f",
                "/bin/bash"
            ]
        }
    ]
}